{
	"name": "Super Styles",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Write custom CSS/JS to any websites",
	"browser_action": {
		"default_icon": "img/logo-32.png",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"storage"
	],
	"icons": {
		"16": "img/logo-16.png",
		"32": "img/logo-32.png",
		"48": "img/logo-48.png",
		"64": "img/logo-64.png",
		"128": "img/logo-128.png",
		"256": "img/logo.png"
	}
}